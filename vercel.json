{
  "buildCommand": "npm install && npm install --prefix frontend && npm run build --prefix frontend",
  "outputDirectory": "frontend/build",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/server.js"
    },
    { "source": "/(.*)", "destination": "/" }
  ]
}
